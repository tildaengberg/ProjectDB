<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - GradeField</title>
    <meta name="application-name" content="Taldok" />
    <meta name="author" content="Lisa, Tilda, Olivia" />
    <meta name="keywords" content="Ladok, Utbildning, Betyg, Kurser, Universitet" />
    <meta name="description" content="En hemsida för att se sina kurser, betyg och annan utbildningsrelaterad information." />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - Lisa Lunqvist, Tilda Engberg, Olivia Harlin
            <a class="text-info" asp-controller="Home" asp-action="Grade" style="float:right;">Betygsätt applikationen</a>
        </div>

    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
